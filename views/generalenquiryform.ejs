<div class="row mt-5">
        <div class="col-md-6 m-auto">
          <div class="card card-body">
            <h1 class="text-center mb-3"><i class="fas fa-sign-in-alt"></i> Leave your Comments and Questions Here</h1>
            <form action="/generalenquiryform" method="POST">
              <div class="form-group">
                    <div class="form-group">
                            <label for="name">Name</label>
                            <input
                              type="name"
                              id="name"
                              name="name"
                              class="form-control"
                              placeholder="Enter Name"
                              value="<%= typeof name != 'undefined' ? name : '' %>"
                            />
                          </div>
                <label for="email">Email</label>
                <input
                  type="email"
                  id="email"
                  name="email"
                  class="form-control"
                  placeholder="Enter Email"
                />
              </div>
              <div class="form-group">
                <label for="phonenumber">phonenumber</label>
                <input
                  type="phonenumber"
                  id="phonenumber"
                  name="phonenumber"
                  class="form-control"
                  placeholder="phonenumber"
                />
              </div>
              <div class="form-group">
                    <label for="generaltext">questions,comments</label>
                    <input
                      type="generaltext"
                      id="generaltext"
                      name="generaltext"
                      class="form-control"
                      placeholder="Questions,and comments go here"
                    />
                  </div>
              <button type="submit" class="btn btn-primary btn-block">Submit Form</button>
              <a href="/User">Register</a>
              <a href="/Session">Login</a>
            </form>
          </div>
        </div>
      </div>